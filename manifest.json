{

    "manifest_version": 2,
    "name": "Proxy",
    "description": "Proxy on 127.0.0.1:8910",
    "version": "1.0",
    "browser_action": {
        "default_popup":"popup.html"
      },
  "permissions": [
      "tabs",
      "http://*/*",
      "https://*/*",
      "notifications",
      "contextMenus",
      "history",
      "background",
      "proxy"
  ],
  "content_scripts":[
    {
        "matches":["https://*/*","http://*/*"],
        "js":["background.js"]
    }
],
"web_accessible_resources":[
    "background.js"
    ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }